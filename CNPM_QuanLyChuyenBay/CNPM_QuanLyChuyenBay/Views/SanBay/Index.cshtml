@model IEnumerable<CNPM_QuanLyChuyenBay.Models.SanBay>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";

}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaSanBay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenSanBay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenThanhPho)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VietTatSanBay)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaSanBay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenSanBay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenThanhPho)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VietTatSanBay)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.MaSanBay }) |
            @Html.ActionLink("Details", "Details", new { id = item.MaSanBay }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.MaSanBay })
        </td>
    </tr>
}

</table>
