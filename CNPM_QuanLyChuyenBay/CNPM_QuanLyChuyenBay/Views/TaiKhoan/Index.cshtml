@model IEnumerable<CNPM_QuanLyChuyenBay.Models.TaiKhoan>

@{
    ViewBag.Title = "Index";
}

<h2>Danh sách tài khoản</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaTaiKhoan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenTaiKhoan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MatKhau)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MaTaiKhoan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenTaiKhoan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MatKhau)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.MaTaiKhoan }) |
            @Html.ActionLink("Details", "Details", new { id = item.MaTaiKhoan }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.MaTaiKhoan })
        </td>
    </tr>
}

</table>
